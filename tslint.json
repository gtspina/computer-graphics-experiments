{
	"defaultSeverity": "warning",
	"extends": [
		"tslint:latest"
	],
	"jsRules": {},
	"rules": {
		"eofline": false,
		"indent": {
			"options": "tabs"
		},
		"no-bitwise": false,
		"no-console": false,
		"no-empty": false,
		"quotemark": {
			"options": "single"
		},
		"semicolon": {
			"options": "never"
		},
		"variable-name": {
			"options": ["allow-leading-underscore"]
		}
	},
	"rulesDirectory": []
}